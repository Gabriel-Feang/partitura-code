    },
    {
      "name": "Types and Classes",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#E5C890"
      }
    },
    {
      "name": "Interfaces",
      "scope": ["entity.name.type.interface"],
      "settings": {
        "foreground": "#5BB8C4"
      }
    },
    {
      "name": "Enums",
      "scope": ["entity.name.type.enum"],
      "settings": {
        "foreground": "#5BB8C4"
      }
    },
    {
      "name": "Namespaces / Modules",
