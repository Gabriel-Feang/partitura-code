  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#6E5A78",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.other",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#5BB8C4"
      }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#5BB8C4"
      }
    },
    {
      "name": "Strings",
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#A6D189"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#E78284"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#5BB8C4"
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#E5C890"
      }
    },
    {
      "name": "Language Constants",
      "scope": ["constant.language"],
      "settings": {
        "foreground": "#E5C890",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Character Constants",
      "scope": ["constant.character", "constant.other"],
      "settings": {
        "foreground": "#E5C890"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call entity.name.function"
      ],
      "settings": {
        "foreground": "#8CAAEE"
